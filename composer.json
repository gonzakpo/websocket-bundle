{
  "name": "gonzakpo/websocket-bundle",
  "type": "symfony-bundle",
  "description": "Ratchet abstraction for Symfony 4",
  "keywords": ["websocket", "symfony"],
  "license": "MIT",
  "authors": [
    {
      "name": "Gonzalo Alonso",
      "email": "gonkpo@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "symfony/framework-bundle": "^3.4|^4.3|^5.0",
    "cboden/ratchet": "^0.4.1",
    "ratchet/pawl": "^0.3.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": { "RollandRock\\WebsocketBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": { "RollandRock\\WebsocketBundle\\Tests\\": "Tests" }
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true"
    }
  }
}
